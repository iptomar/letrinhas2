<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.letrinhas2" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<preference name="SplashScreen" value="screen"/>
<preference name="SplashScreenDelay" value="5100"/>
<preference name="Fullscreen" value="true" />
	 <platform name="android">
    <!-- you can use any density that exists in the Android project -->
    <splash src="screen/splash-land-hdpi.png" density="land-hdpi"/>
    <splash src="screen/splash-land-ldpi.png" density="land-ldpi"/>
    <splash src="screen/splash-land-mdpi.png" density="land-mdpi"/>
    <splash src="screen/splash-land-xhdpi.png" density="land-xhdpi"/>
    
    <splash src="screen/splash-port-hdpi.png" density="port-hdpi"/>
    <splash src="screen/splash-port-ldpi.png" density="port-ldpi"/>
    <splash src="screen/splash-port-mdpi.png" density="port-mdpi"/>
    <splash src="screen/splash-port-xhdpi.png" density="port-xhdpi"/>

  </platform>


    <name>Letrinhas2</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
